{
  "name": "@cdmbase/i18next",
  "version": "0.0.1",
  "description": "npm package for i18next",
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "author": "",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*test*\\.(ts|tsx|js)$"
  },
  "dependencies": {
    "i18next": "^10.0.5"
  },
  "devDependencies": {
    "@types/i18next": "^8.4.2",
    "@types/jest": "^20.0.8",
    "@types/node": "^8.0.47",
    "jest": "^21.0.1",
    "ts-jest": "^21.0.0",
    "typescript": "^2.5.2"
  }
}
